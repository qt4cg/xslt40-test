   <description>Tests default priority of patterns</description>
 

   <test-case name="priority-001">
      <description>
         Test default priority of pattern .
      </description>
      <created by="Michael Kay" on="2025-12-02"/>
      <dependencies>
         <spec value="XSLT30+"/>
      </dependencies>
      <test>
         <stylesheet file="priority-A.xsl"/>
         <param static="true" name="match" select="'.'"/>
         <param static="true" name="select" select="'42'"/>
         <param static="true" name="expected" select="-1"/>
      </test>
      <result>
         <assert>/A/B/C</assert>
      </result>
   </test-case>
   
