<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns="http://www.w3.org/2012/10/xslt-test-catalog" xmlns:xs="http://www.w3.org/2001/XMLSchema" 
   name="value-of" covers-40="PR2251">
   <description>Tests xsl:value-of</description>

   
   <test-case name="value-of-001">
      <description>
         xsl:value-of with select attribute
      </description>
      <created by="Michael Kay" on="2025-10-21"/>
      <dependencies>
         <spec value="XSLT40+"/>
      </dependencies>
      <test>
         <stylesheet file="value-of-001.xsl"/>
      </test>
      <result>
         <all-of>
            <assert>/out/a = 'abc'</assert>
            <assert>/out/b = '42'</assert>
            <assert>/out/c = '2025-10-21'</assert>
            <assert>/out/d = 'true'</assert>
            <assert>/out/e = 'true'</assert>
            <assert>/out/f = '23'</assert>
            <assert>/out/g = 'true'</assert>
            <assert>/out/h = ''</assert>
            <assert>/out/i = 'true'</assert>
            <assert>/out/j = '1'</assert>
         </all-of>
      </result>
   </test-case>
   
